{
  "name": "llm-survey-analysis",
  "version": "1.0.0",
  "description": "Implementation of Large Language Models for Software Engineering Survey Analysis",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python -m uvicorn main:app --reload --port 8000",
    "install:all": "npm install && cd frontend && npm install",
    "build": "cd frontend && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "LLM",
    "survey-analysis",
    "software-engineering",
    "AI",
    "sentiment-analysis"
  ],
  "author": "",
  "license": "MIT"
}
